{
  "compilerOptions": {
    "target": "es6",
        "lib": [
      "esnext",
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
        "*": [
            "node_modules/*"
        ]
    }
  }
}
